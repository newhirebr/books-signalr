<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
  <base-config cleartextTrafficPermitted="true">
    <trust-anchors>
      <!-- Include your own trusted certificates here -->
      <!-- Remove this line if you're not using self-signed certificates -->
      <certificates src="system" />
    </trust-anchors>
  </base-config>
</network-security-config>